<script type="text/javascript">

    var WarehouseAccess = @Html.Raw(Json.Encode(@HttpContext.Current.Session["WarehouseAccess"]));

</script>



<script type="text/javascript">

    //Search
    $(document).ready(function () {
        $('#DocDate').datepicker({
            autoclose: true
        });

        //Date + time
        var d = new Date();
        var month = d.getMonth() + 1;
        var day = d.getDate();

        var output =
            (('' + day).length < 2 ? '0' : '') + day + '/' +
            (('' + month).length < 2 ? '0' : '') + month + '/' +
            (parseInt(d.getFullYear()) + 543)

        $("#DocDate").val(output);


        $("#btnOpenModal").click(function (e) {
            e.defaultPrevented;
            if ($("#WhID").val().trim() == "") {
                $("span[data-valmsg-for='WhID']").html("กรุณาเลือกสำนักงานขนส่ง");
            } else {
                $("span[data-valmsg-for='WhID']").html("");
                $("#ListModal").modal("show");
            }

        });

        $("#btnSelWh").click(function (e) {
            e.defaultPrevented;
            //Get Staff in warehouse
            $.ajax({
                type: "POST",
                url: "@Url.Action("GetWH")",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (response) {
                    //console.log(response);
                    if (response.length > 0) {
                        $('#modSelToWH tbody').empty();
                        for (var x = 0; x < response.length; x++) {
                            var newRow = '<tr>';
                            newRow = newRow + '<td align="left">' + response[x].Name + '</td>';
                            newRow = newRow + '<td align="right">' + '<a href="#"><i name="SelTW" TowhCode="' + response[x].ID + '" TowhName="' + response[x].Name + '" class="fa fa-fw fa-pencil-square fa-2x" onclick="selWH(event,this)" ></i></a>' + '</td>';
                            newRow = newRow + '</tr>';

                            $('#modSelToWH tbody').append(newRow);
                        }
                    }
                },
                error: function (response) {
                    console.log(response)
                }
            });

            $("#TWModal").modal("show");

        });


        $("#ProductList").on("change", function () {
            var prdID = $(this).val();
            var whID = $("#WhID").val();

            if (prdID == "") {
                $("#PrdErrMsg").html("");

                $("#Qty").val("");
                $("#Qty").removeAttr("readonly");
                $("#Qty").css("background-color", "");
            } else {


                //check stock available
                @*$.ajax({
                    type: "GET",
                    url: "@Url.Action("checkQtyByProduct")" + "?WhID=" + whID + "&prdID=" + prdID,
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (response) {
                        //console.log(response);

                        // y=available | n=not available
                        if (response == "n") {

                            $("#ProductList").val("");
                            $("#PrdErrMsg").html("วัสดุมีจำนวนคงเหลือไม่พอ");
                            $("#PrdErrMsg").css("display", "");

                            $("#Qty").val("");
                            $("#Qty").removeAttr("readonly");
                            $("#Qty").css("background-color", "");

                            $("#isAsset").val("n");

                        } else {
                            $("#PrdErrMsg").html("");*@


                            // check is assets
                            $.ajax({
                                type: "GET",
                                url: "@Url.Action("checkIsAssetSNControl")" + "?prdID=" + prdID,
                                contentType: "application/json; charset=utf-8",
                                dataType: "json",
                                success: function (response) {
                                    //console.log(response);
                                    if (response.isAsset == "y") {
                                        $("#isAsset").val("y");
                                        $("#Qty").val("1");
                                        $("#serialControl").val(response.isControlSerial);

