                                        $("#editQty").val("");
                                        $("#editQty").removeAttr("readonly");
                                        $("#editQty").css("background-color", "");

                                        $("#editSerialEnd").removeAttr("readonly");

                                        $("#editIpProperty").val("");
                                        $("#editIpProperty").attr("readonly", "readonly");
                                    }

                                },
                                error: function (response) {
                                    console.log(response)
                                }


                            });
                        }

                //    },
                //    error: function (response) {
                //        console.log(response)
                //    }
                //});
            //}
