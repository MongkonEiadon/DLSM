            row += '<td align="center" style="text-align:center">' + btnDel + '</td>';
            row += '<td align="center" style="text-align:center;display:none;">' + mdIsAsset + '</td>';
            row += '</tr >';

            //Append new row
            $("#tbMain > tbody").last().append(row);

