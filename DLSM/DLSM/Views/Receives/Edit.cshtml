    function checkDuplicateSNRow(modalType) {

        var ret = true;

        var RowNo = "";
        var PrdID = "";
        var SNBeg = "";
        var SNEnd = "";

        // get new detail
        if (modalType == "add") {
            RowNo = $("#RowNo").val();
