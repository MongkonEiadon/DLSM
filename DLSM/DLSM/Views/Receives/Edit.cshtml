            $("#Qty").val("");
            $("#SerialBegin").val("");
            $("#SerialEnd").val("");
            $("#IpProperty").val("");
            $("#TotalQty").val("");
            $("#isAsset").val("n");


            //Close Modal
            $("#ListModal").modal("hide");
        });

